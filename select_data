SELECT 
    T_JKC_EMAIL.DS_EMAIL      "EMAIL", 
    T_JKC_EMPRESA.DS_RAZAO_SOCIAL     "EMPRESA",
    T_JKC_EMPRESA.NR_CNPJ,
    T_JKC_TELEFONE.NR_DDD      "DDD",
    T_JKC_TELEFONE.NR_TELEFONE     "TELEFONE",
    T_JKC_LOGRADOURO.DS_LOGRADOURO      "RUA",
    T_JKC_EMP_LOGR.NR_ENDERECO      "NÃšMERO",
    T_JKC_BAIRRO.NM_BAIRRO      "BAIRRO",
    T_JKC_CIDADE.NM_CIDADE      "CIDADE",
    T_JKC_AGENDA_VISITA.DT_AGENDA     "DATA VISITA"
    
FROM 
    T_JKC_EMPRESA 
INNER JOIN T_JKC_EMP_EMAIL
    ON T_JKC_EMP_EMAIL.ID_EMPRESA = T_JKC_EMPRESA.ID_EMPRESA
INNER JOIN T_JKC_EMAIL 
    ON T_JKC_EMP_EMAIL.ID_EMAIL = T_JKC_EMAIL.ID_EMAIL
INNER JOIN T_JKC_EMP_TELEF 
    ON T_JKC_EMPRESA.ID_EMPRESA = T_JKC_EMP_TELEF.ID_EMPRESA
INNER JOIN T_JKC_TELEFONE
    ON T_JKC_EMP_TELEF.ID_TELEFONE = T_JKC_TELEFONE.ID_TELEFONE
INNER JOIN T_JKC_EMP_LOGR
    ON T_JKC_EMPRESA.ID_EMPRESA = T_JKC_EMP_LOGR.ID_EMPRESA
INNER JOIN T_JKC_AGENDA_VISITA
    ON T_JKC_EMP_LOGR.ID_LOGR_EMPRESA = T_JKC_AGENDA_VISITA.ID_LOGR_EMPRESA
INNER JOIN T_JKC_LOGRADOURO
    ON T_JKC_EMP_LOGR.ID_LOGRADOURO = T_JKC_LOGRADOURO.ID_LOGRADOURO
INNER JOIN T_JKC_BAIRRO 
    ON T_JKC_LOGRADOURO.ID_BAIRRO = T_JKC_BAIRRO.ID_BAIRRO
INNER JOIN T_JKC_CIDADE
    ON T_JKC_BAIRRO.ID_CIDADE = T_JKC_CIDADE.ID_CIDADE
    
ORDER BY T_JKC_AGENDA_VISITA.DT_AGENDA;
